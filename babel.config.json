{
  "presets": ["@babel/preset-env" ,"@babel/preset-typescript"],
  "plugins": [
    ["module-resolver", {
      "root": ["./"],
      "alias": {
        "@": "./src"
      }
    }]
  ],
  "ignore": ["build", "node_modules",
   "**/tests/unit/**/*.spec.[jt]s?(x)",
    "**/__tests__/**/*.spec.[jt]s?(x)"
  ],
}